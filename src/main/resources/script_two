SELECT COMPANY.NAME AS COMPANY_NAME, COUNT(COMPANY_ID) AS COUNT_PERSON FROM PERSON
LEFT JOIN COMPANY ON COMPANY.ID = COMPANY_ID
GROUP BY COMPANY_NAME
ORDER BY COUNT_PERSON DESC  LIMIT 1




